}

QPushButton:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1, 
                                stop:0 #62A9F4, 
                                stop:1 #2A73D9);
    border: 2px solid #2A73D9;
}

QPushButton:pressed {
    background: #1C62B9;
    border: 2px solid #0E4A91;
}

/* Text Input Fields */
QLineEdit {
    background: rgba(255, 255, 255, 0.2);
    border: 2px solid #5A89C4;
    padding: 6px;
}

/* Table/Grid View */
