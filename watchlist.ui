}

/* Grid Items (Labels, Buttons, Inputs) */
QLabel, QPushButton, QLineEdit {
    background: rgba(255, 255, 255, 0.1); /* Semi-transparent */
    color: white;
    font-size: 14px;
    font-weight: bold;
    padding: 8px;
    border-radius: 5px;
    border: 2px solid rgba(255, 255, 255, 0.2); /* Prevents border overlap */
}

/* Grid Button Styling */
